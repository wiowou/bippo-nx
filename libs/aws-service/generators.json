{
  "$schema": "http://json-schema.org/schema",
  "name": "aws-service",
  "version": "0.0.1",
  "generators": {
    "step-function": {
      "factory": "./src/generators/step-function/generator",
      "schema": "./src/generators/step-function/schema.json",
      "description": "step-function generator"
    },
    "lambda-api": {
      "factory": "./src/generators/lambda-api/generator",
      "schema": "./src/generators/lambda-api/schema.json",
      "description": "lambda-api generator"
    },
    "lambda": {
      "factory": "./src/generators/lambda/generator",
      "schema": "./src/generators/lambda/schema.json",
      "description": "lambda generator"
    },
    "preset": {
      "factory": "./src/generators/preset/generator",
      "schema": "./src/generators/preset/schema.json",
      "description": "preset generator",
      "x-use-standalone-layout": true
    },
    "init": {
      "factory": "./src/generators/init/generator",
      "schema": "./src/generators/init/schema.json",
      "description": "init generator"
    }
  }
}
