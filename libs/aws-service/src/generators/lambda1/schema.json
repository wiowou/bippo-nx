{
  "$schema": "http://json-schema.org/schema",
  "cli": "nx",
  "$id": "Lambda1",
  "title": "",
  "type": "object",
  "properties": {
    "name": {
      "type": "string",
      "description": "",
      "$default": {
        "$source": "argv",
        "index": 0
      },
      "x-prompt": "What name would you like to use?"
    },
    "awsProfile": {
      "type": "string",
      "description": "The AWS profile name you plan to use to locally create infra"
    },
    "terraformVersion": {
      "type": "string",
      "description": "min version of Terraform installer"
    },
    "terraformAwsVersion": {
      "type": "string",
      "description": "min version of Terraform AWS provider"
    },
    "database": {
      "type": "string",
      "description": "generate files related to this database",
      "enum": ["dynamo"],
      "default": "dynamo"
    }
  },
  "required": ["name"]
}
